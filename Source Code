#include<stdio.h>
#include<conio.h>
int main()
{

int s,v,t;
printf("PROOGRAM MENGHITUNG JARAK TEMPUH\n");
printf("================================");
printf("INSTITUT PELITA BANGSA. TEKNIK INFORMATIKA\n\n");
printf("Masukkan kecepatan (v) : ");scanf("%i",&v);
printf("Masukkan waktu dalam sekon (t) : ");scanf("%i",&t);
s=v*t;
printf("S = %d * %d = %i Meter",v,t,s);
getch();
}

